{
    "name": "maxwelladapoe/laravel-password-is-too-common",
    "description": "A simple common password checker package",
    "keywords": [
        "maxwelladapoe",
        "passwords",
        "security",
        "common",
        "too common"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxwell Adapoe",
            "email": "me@maxwelladapoe.com",
            "homepage": "https://www.maxwelladapoe.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.1",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "MaxwellAdapoe\\PasswordIsTooCommon\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "MaxwellAdapoe\\PasswordIsTooCommon\\PasswordIsTooCommonServiceProvider"
            ],
            "aliases": {
                "PasswordIsTooCommon": "MaxwellAdapoe\\PasswordIsTooCommon\\Facads\\PasswordIsTooCommon"
            }
        }
    }
}
